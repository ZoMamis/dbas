<div metal:use-macro="layout">
	<div metal:fill-slot="content_page">

		<head>
			<link type="text/css" href="${request.static_url('dbas:static/css/theme_404_logout.css')}" rel="stylesheet">
		</head>


		<div class="center text-center">
			<div class="error">
				<h1><span class="font-semi-bold" i18n:translate="404">404 Error</span></h1>
				<p class="lead font-normal" i18n:translate="404_text">The page &quot;<span i18n:name="page_notfound_viewname" i18n:translate="" class="font-semi-bold">${page_notfound_viewname}</span>&quot; for could not be found.</p>
