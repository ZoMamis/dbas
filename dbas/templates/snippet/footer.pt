<footer id="footer" i18n:domain="dbas" metal:define-macro="layout">
  <div class="links" style="padding-bottom: 0.5em">
    <ul>
      <li>
        <i class="fa fa-copyright" aria-hidden="true"></i>
        <span>${extras.year} Krauthoff</span>
      </li>
      <li>
        <a href="/imprint">
          <i class="fa fa-copyright" aria-hidden="true"></i><span i18n:translate="imprint">Imprint</span>
        </a>
      </li>
      <li>
        <a href="/privacy_policy">
          <i class="fa fa-user-secret" aria-hidden="true"></i><span i18n:translate="privacy_policy">privacy_policy</span>
        </a>
      </li>
      <li id="contact-link">
        <a href="mailto:dbas@cs.uni-duesseldorf.de">
          <i class="fa fa-comment" aria-hidden="true"></i><span i18n:translate="contact">Contact</span>
        </a>
      </li>
      <li>
        <a href="/graphiql">
          <i class="fa fa-circle-o" aria-hidden="true"></i><span>GraphiQL</span>
        </a>
      </li>
    </ul>
    <ul>
      <li>
        <a href="/rss">
          <i class="fa fa-rss" aria-hidden="true"></i><span i18n:translate="rss">RSS</span>
        </a>
      </li>
      <li>
        <a href="/faq">
          <i class="fa fa-question" aria-hidden="true"></i><span i18n:translate="Help">Help</span>
        </a>
      </li>
      <li>
        <a href="/docs">
          <i class="fa fa-book" aria-hidden="true"></i><span i18n:translate="Documentation">Docs</span>
        </a>
      </li>
      <li>
        <a href="/news" data-toggle="tooltip" data-placement="left">
          <i class="fa fa-newspaper-o" aria-hidden="true"></i><span i18n:translate="news">News</span>
        </a>
      </li>
      <li>
        <a href="https://github.com/hhucn/dbas">
          <i class="fa fa-github" aria-hidden="true"></i><span i18n:translate="Github">Github</span>
        </a>
      </li>
      <li>
        <a href="/fieldexperiment">
          <i class="fa fa-flask" aria-hidden="true"></i><span i18n:translate="field_experiment">Field Experiment</span>
        </a>
      </li>
      <li id="roundhousekick-wrapper" style="position: absolute; margin: 0; right: 25px;">
        <img id="roundhousekick"
             alt=".r"
             style="width: 25px; height: 25px; border: none; opacity: 0;"
             src="${request.static_url('dbas:static/images/transparent.gif')}">
      </li>
      <li id="yomamma-wrapper" style="position: absolute; margin: 0; right: 0;">
        <img id="yomamma" alt=".y"
             style="width: 25px; height: 25px; border: none; opacity: 0;"
             src="${request.static_url('dbas:static/images/transparent.gif')}">
      </li>
    </ul>
  </div>
</footer>
