    <!-- LOGIN POPUP for LDAP-->
    <div tal:condition="extras.use_with_ldap" class="modal fade" id="popup-login" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLoginLDAP" aria-hidden="true">
