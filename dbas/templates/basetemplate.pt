<html lang="en" xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="dbas" metal:define-macro="layout" tal:attributes="xml:lang extras.lang">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="dialog based discussion online participation">
	<meta name="author" content="Tobias Krauthoff">
	<link rel="shortcut icon" href="${request.static_url('dbas:static/images/favicon-16x16.png')}">
	<title>${project} - ${title}</title>
	<!-- Bootstrap core CSS -->
	<link type="text/css" href="${request.static_url('dbas:static/css/bootstrap-3.3.6-paper.min.css')}" rel="stylesheet">
	<link type="text/css" href="${request.static_url('dbas:static/css/bootstrap-toggle.min.css')}" rel="stylesheet">
	<link type="text/css" href="${request.static_url('dbas:static/css/theme_basetemplate.css')}" rel="stylesheet">
	<link type="text/css" href="${request.static_url('dbas:static/css/theme_form.css')}" rel="stylesheet">
	<link type="text/css" href="${request.static_url('dbas:static/css/customnavbar.css')}" rel="stylesheet">
	<title>${project} - ${title}</title>
</head>

<body>
  <img src="${request.static_url('dbas:static/images/alpha.png')}" alt="alpha logo" style="position:fixed; z-index:10; top:65px; width:100px;">
	<input type="hidden" id="hidden_language" value="${language}">
    <input type="hidden" id="hidden_csrf_token" name="csrf_token" value="${request.session.get_csrf_token()}">

	<!-- Inlcuding this at top for references -->
	<script type="text/javascript" src="${request.static_url('dbas:static/js/jquery-2.2.3.min.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/bootstrap-3.3.6.min.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/bootstrap-toggle.min.js')}"></script>
	<!--
	<script type="text/javascript" src="${request.static_url('dbas:static/js/ads.js')}"></script>
	-->

	<!-- JS DISABLED -->
	<noscript>
		<div class="blocked jsdisabled">
			<strong i18n:translate="js_disabled1">JavaScript is not enabled:</strong><br>
			<strong i18n:translate="js_disabled2">Please enable JavaScript!</strong>
		</div>
	</noscript>
	<!--
	<div id="adblocker_message" class="blocked adblockenabled" style="display:none">
		<strong i18n:translate="adblock_enabled1">AdBlocker is enabled:</strong><br>
