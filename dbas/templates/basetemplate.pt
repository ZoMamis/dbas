	</div>

	<script type="text/javascript">
	  if( window.canRunAds === undefined ){
		// adblocker detected, show fallback
		$('#adblocker_message').show();
	  }
	</script>
	-->


	<div class="blocked request_failed center alert-danger" id="request_failed_container" style="display:none;">
		<h5 i18n:translate="request_failed">Your request failed!</h5>
        <h6 i18n:translate="please_reload">Please reload the page.</h6>
	</div>

	<div id="additional-wrapper">
		<div id="wrapper-header">
			<div metal:use-macro="load: snippet-header.pt"></div>
			<div metal:use-macro="load: snippet-popups.pt"></div>
		</div>

		<div id="wrapper-content">
			<div metal:define-slot="content_page"></div>
		</div>
		
		<div id="wrapper-footer">
			<a href="#top" class="back-to-top"><img class="center" id="back-to-top-img" src="${request.static_url('dbas:static/images/home.png')}"></a>

			<div metal:use-macro="load: snippet-footer.pt"></div>
		</div>

		<div class="ajaxloader"></div>
	</div>

	<div class="eupopup eupopup-bottomleft"></div>

	<!-- Placed at the end of the document so the pages load faster -->
	<!--<script type="text/javascript" src="${request.static_url('dbas:static/js/pace.min.js')}"></script>-->
	<script type="text/javascript" src="${request.static_url('dbas:static/js/jquery.slimscroll-1.3.7.min.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/ids.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/settings.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/app.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/sharing.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/strings.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/discussion/discussion-guihandler.js')}"></script>
	<script type="text/javascript" src="${request.static_url('dbas:static/js/discussion/discussion-helper.js')}"></script>
	<!--<script type="text/javascript" src="${request.static_url('dbas:static/js/gremlins.min.js')}"></script>-->

	<!-- Piwik -->
	<script type="text/javascript">
		var _paq = _paq || [];
		_paq.push(['trackPageView']);
		_paq.push(['enableLinkTracking']);
		(function () {
			var u = "//dbas.cs.uni-duesseldorf.de/piwik/";
			_paq.push(['setTrackerUrl', u + 'piwik.php']);
			_paq.push(['setSiteId', 1]);
			var d = document,
				g = d.createElement('script'),
				s = d.getElementsByTagName('script')[0];
			g.type = 'text/javascript';
			g.async = true;
			g.defer = true;
			g.src = u + 'piwik.js';
			s.parentNode.insertBefore(g, s);
		})();
	</script>
	<noscript>
		<p>
			<img src="//dbas.cs.uni-duesseldorf.de/piwik/piwik.php?idsite=1" style="border:0;" alt=""/>
		</p>
	</noscript>
	
