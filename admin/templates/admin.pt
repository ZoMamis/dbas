<tal:block metal:use-macro="layout" i18n:domain="dbas" metal:define-macro="layout">
	<tal:block metal:fill-slot="content_page">
		
		<div class="container" tal:condition="not:extras.is_admin" style="max-width: 578px;">
            <span class="lead">401 - Unauthorized</span><br>
			<img class="center" src="${request.static_url('dbas:static/images/nopower.jpg')}">
		</div>

        <div class="container" tal:condition="extras.is_admin">

            <div class="row" tal:repeat="row dashboard">

                <div class="col-md-3" tal:repeat="group row">

                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">${group.name}</h3>
                        </div>
                        <div class="panel-body" tal:repeat="line group.content">
                            <a href="" tal:attributes="href line.href"><p>${line.name}  <span class=" badge">${line.count}</span></p></a>
                        </div>
                    </div>

                </div>

            </div>
        </div>

		<script type="text/javascript" src="${request.static_url('dbas:static/js/discussion/helper.js')}"></script>
		<script type="text/javascript" src="${request.static_url('dbas:static/js/discussion/guihandler.js')}"></script>

	</tal:block>
</tal:block>